{
  "name": "pwa-offline",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all --parallel sw-dev-webpack start",
    "start": "ng serve -o",
    "build-prod": "ng build --prod",
    "postbuild-prod": "npm run sw-prod-webpack",
    "test": "ng test",
    "lint": "ng lint",
    "sw-prod-webpack": "rimraf ./dist/pwa-offline/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --colors && workbox injectManifest ./workbox-config.js",
    "serve-dist": "ws --hostname localhost -d dist/pwa-offline -p 1234 -o --log.format stats",
    "sw-dev-webpack": "rimraf ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@angular/service-worker": "~10.1.0",
    "copy-webpack-plugin": "^6.1.1",
    "dexie": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "workbox-core": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-window": "^5.1.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "local-web-server": "^4.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "workbox-cli": "^5.1.4"
  }
}
